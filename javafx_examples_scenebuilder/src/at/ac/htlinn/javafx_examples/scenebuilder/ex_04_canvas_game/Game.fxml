<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.htlinn.javafx_examples.scenebuilder.ex_04_canvas_game.GameController">
   <center>
      <Canvas fx:id="field" height="800.0" width="1000.0" BorderPane.alignment="CENTER" />
   </center>
   <right>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="startBtn" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Stop" />
            <Label fx:id="noMonsterLbl" maxWidth="1.7976931348623157E308" text="Monster">
               <VBox.margin>
                  <Insets top="40.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="feedbackLbl" text="Label" />
         </children>
      </VBox>
   </right>
</BorderPane>
